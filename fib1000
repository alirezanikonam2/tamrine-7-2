#include<iostream>
using namespace std;
int fibonachy(int x){
	if (x <= 1){
		return x;
	}
	return fibonachy(x - 1) + fibonachy(x - 2);



}
int main(){
	int x;
	cout << "this app fibonachy" << endl;
	
	for (int i = 0; i < 1000; i++){
		cout << fibonachy(i)<<" " ;
	}
	




	system("pause");
	return 0;
}
